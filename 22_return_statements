# return = statement used to end a function
#          and send a result back to the caller

def add(x, y):
    z = x + y
    return z

def subtract(x, y):
    z = x - y
    return z

def multiply(x , y):
    z = x * y
    return z

def divide(x, y):
    z = x / y
    return z

print(add(1, 3))
print(subtract(5, 10))
print(multiply(3, 5))
print(divide(3, 5))
