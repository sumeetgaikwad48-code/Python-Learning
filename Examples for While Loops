food = input("Enter the food you like (q to quit): ")

#Example - 1

while not food  == "q":
    print(f"You like {food}")
    food = input("Enter the food you like (q to quit): ")
print("BYE")

#Example - 2

num = int(input("Enter a # between 1 - 10: "))

while num < 1 or num > 10:
    print(f"{num} is not valid")
    num = int(input("Enter a # between 1 - 10: "))
print(f"your number is {num}")
